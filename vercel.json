{
    "version": 1,
    "build": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/schemas/**/*.graphql"
                ]
            }
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}